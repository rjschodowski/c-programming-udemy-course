/*
Write a program to find the smallest of two numbers.
*/

#include <stdio.h>

void findSmallestNumber(int num1, int num2)
{
  if(num1>num2)
  {
    printf("%d is the smallest number.", num2);
  } else if (num1<num2)
  {
    printf("%d is the smallest number.", num1);
  } else
  {
    printf("%d and %d are equal in value!", num1, num2);
  }
}

int main() {
  int number1, number2;
  printf("Enter two numbers: \n");
  scanf("%d %d", &number1, &number2);
  findSmallestNumber(number1, number2);
  return 0;
}
